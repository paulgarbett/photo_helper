<!DOCTYPE html>
<html>
<head>
  <title>PhotoHelper</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
   <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>

	<link href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap-glyphicons.css" rel="stylesheet">
</head>
<body>
	<div id="container">

	<nav class="navbar navbar-default navbar-fixed-top" role="navigation">
	  <div class="container">
			  <!--logout -->
		  <div id="user_header">
		    <% if @current_user %>
		      Logged in as <%= @current_user.email %>.
		      <%= link_to "Log Out", logout_path(current_user) %>
		    <% else %>
		       <button type="button" class="btn btn-default navbar-btn"><%= link_to "Sign Up", new_user_path %></button> 
		      <button type="button" class="btn btn-default navbar-btn"><%= link_to "Log In", new_session_path %></button>
		    <% end %>
			</div>
	  </div>
	</nav>


    <button type="button" class="btn btn-default navbar-btn">Sign in</button>
	  
  </div>

  

	<%= yield %>

</body>
</html>
